                <component
                  refId="Package\Contenedor de bucles Foreach\Data Flow Task\Origen de ADO NET"
                  componentClassID="Microsoft.ManagedComponentHost"
                  contactInfo="Consume datos de SQL Server, OLE DB, ODBC u Oracle mediante el correspondiente proveedor de datos de .NET Framework. Se usa una instrucción Transact-SQL para definir el conjunto de resultados. Por ejemplo, se extraen datos de SQL Server con el proveedor de datos de .NET Framework para SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; Todos los derechos reservados; http://www.microsoft.com/sql/support;4"
                  description="Consume datos de SQL Server, OLE DB, ODBC u Oracle mediante el correspondiente proveedor de datos de .NET Framework. Se usa una instrucción Transact-SQL para definir el conjunto de resultados. Por ejemplo, se extraen datos de SQL Server con el proveedor de datos de .NET Framework para SQL Server."
                  name="Origen de ADO NET"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @FechaInicio VARCHAR(50),  @FechaFin VARCHAR(50);
/****RANGO DE FECHAS DE FACTURACION*********/

set @FechaInicio= '2021-09-01 0:00:00'  --/----------------FECHA DE INICIO/
set  @FechaFin=  '2021-09-11 23:59:00'  --/----------------FECHA DE FINALIZACION/  

declare @tienda int
declare @nombretienda VARCHAR(50)
declare @nombretiendafactura VARCHAR(50)
select @nombretienda= fcTdaNombre from control
select @nombretiendafactura= fcTdaNombre from CATFISControl
select @tienda= fiNoTienda from control

--FACTURAS DE VENTA DE CONSUMO*******'/
select
DISTINCT  @tienda as 'Tienda',
factura.fccodigohash as 'CodigoHash',
factura.fdcreacion as 'FechaCreacion',
factura.FCFOLIO,

case factura.fiestatusid
WHEN 0 THEN 'Generado'
WHEN 1 THEN 'Procesado Por Aplicacion'
WHEN 3 THEN 'En contingencia'
WHEN 4 THEN 'Procesado por Tarea'
ELSE 'No encontrado'
END as 'EstatusFactura',  

case factura.fitipoid
WHEN 1 THEN 'Factura'
WHEN 7 THEN 'Nota de Credito'
ELSE 'No encontrado'
END as 'TipoDocumento',  

@nombretienda as 'SucursalADN',
@nombretiendafactura as 'SucursalFacturacion',
factura.fcnoreferencia as 'VentaPedido',

case pedido.fitipoventa
WHEN 1 THEN 'VENTA CONTADO     '
WHEN 2 THEN 'VENTA CREDITO     '
WHEN 3 THEN 'VENTA DE APARTADO '
WHEN 4 THEN 'VENTA MOSTRADOR   '
WHEN 5 THEN 'CREDITO PLUS      '
WHEN 6 THEN 'CRED.PRES UNEFON  '
WHEN 7 THEN 'POR CANC.(e-comm) '
WHEN 8 THEN 'CRED. HIPOTECARIO '
WHEN 9 THEN 'ARRENDAMIENTO PURO'
WHEN 10 THEN 'LINEA CORPORATIVA '
WHEN 14 THEN 'CREDITO T.A. IUSA '
ELSE 'VENTA DE TIEMPO DE AIRE'
END 'SurtimientoTipo',

pedido.fdFecSurt as 'FechaSurtimiento',

case pedido.fipedstat
WHEN 1 THEN  'SURTIDO'
WHEN 2 THEN 'CANCELADO ANTES DE ENTREGA'
WHEN 3 THEN 'CANCELADO DESPUES DE ENTREGA'
WHEN 4 THEN 'CONGELADO'
WHEN 5 THEN 'APARTADO'
ELSE 'NO SE ENCONTRO INFORMACION'
END 'EstadoPedido',

case
WHEN cliente.fcCteFisMor='1' OR cliente.fcCteFisMor= 'FI' THEN 'CLIENTE FISICO'
WHEN cliente.fcCteFisMor= 'MO' THEN 'EMPRESA'
ELSE 'No Encontrado'
END 'TipoPersona',


CONCAT(RTRIM(LTRIM(fcCteNombre)),' ',RTRIM(LTRIM(fcCteAPaterno)),' ',RTRIM(LTRIM(fcCteAMaterno))) AS Nombrefactura,

CASE
When cliente.fcCteDirCalle is NULL  Then 'CIUDAD'
else cliente.fcCteDirCalle
end 'Direccion Cliente',


case factura.finegocioid
WHEN 1 THEN 'Consumo'
WHEN 2 THEN 'Tiempo de aire'
ELSE 'No encontrado'
END as 'TipoVenta',

tipo_operacion.fcTopDesc,

case 
WHEN cliente.fcCteRFC IS NULL THEN 'CF'
when cliente.fcCteRFC=' ' then 'CF'
when cliente.fcCteRFC like '%RFC%' then 'CF'
when cliente.fcCteRFC like '%RCF%' then 'CF'
when cliente.fcCteRFC like '%EKT%' then 'CF'
when cliente.fcCteRFC='0' then 'CF'
when cliente.fcCteRFC='1' then 'CF'
else REPLACE(replace(fcCteRFC,'-',''),'/','') 
END as NIT,

producto.fiprodid,
detalle_pedido.fiCantArt,
producto.fcProdDesc,
detalle_pedido.fnPrcUnit,

case 
WHEN pedido.ficteid=99999 THEN detalle_pedido.fnProdDesctoE
ELSE detalle_pedido.fnPdctoDescto
END 'Descuento',

(detalle_pedido.fnPrcUnit-detalle_pedido.fnPdctoDescto-detalle_pedido.fnProdDesctoE)-((detalle_pedido.fnPrcUnit-detalle_pedido.fnPdctoDescto-detalle_pedido.fnProdDesctoE)/1.12) as "IVA",

case 
WHEN pedido.fipedstat=1 THEN (detalle_pedido.fnPrcUnit-detalle_pedido.fnPdctoDescto-detalle_pedido.fnProdDesctoE)
WHEN pedido.fiPedStat=3 THEN -1*(detalle_pedido.fnPrcUnit-detalle_pedido.fnPdctoDescto-detalle_pedido.fnProdDesctoE)
ELSE 'No Encontrado'
END  as 'TotalVenta',

moto.fcMarca,
moto.fcNoSerie,
moto.fcNoMotor,
moto.fcTipoMoto,
moto.fcNoCaja,
moto.fcColor,
moto.fcPedimento,
moto.fiAnio,
moto.fcmodelo,

case
when fcProdDesc like '%GTIA%' OR fcProdDesc like  '%SEGURO%' then 'S'
Else 'B'
END as 'BienOServicio'

from fisbitacoradocfiscal factura  
FULL JOIN pedido ON pedido.finopedido=factura.fcNoReferencia   
FULL JOIN cliente ON cliente.ficteid=pedido.ficteid and cliente.fiDigitoVer=pedido.fiDigitoVer and cliente.finotienda=pedido.finotienda 
FULL JOIN detalle_pedido ON detalle_pedido.finopedido=pedido.finopedido 
FULL JOIN producto ON producto.fiprodid=detalle_pedido.fiprodid 
FULL JOIN surtimientomoto  moto ON moto.fiNoPedido=pedido.finopedido 
FULL JOIN control ON control.fiNoTienda=pedido.finotienda
FULL JOIN ventatarjeta ON ventatarjeta.fcmin=factura.fcnoreferencia 
FULL JOIN movto_caja ON movto_caja.fiNoTransac=ventatarjeta.fiTranNo  
FULL JOIN tipo_operacion ON tipo_operacion.fitipoop=movto_caja.fitipoop
FULL JOIN gbdcartinsert ON  gbdcartinsert.finopedidonue=pedido.finopedido and gbdcartinsert.ficteid=pedido.ficteid and gbdcartinsert.fidigitover=pedido.fidigitover and gbdcartinsert.finotienda=pedido.finotienda
where  --factura.finegocioid=1 AND factura. fdCreacion between @FechaInicio AND @FechaFin       
(factura.finegocioid=1 or factura.finegocioid is null) and (factura.fiestatusid=0 or factura.fiestatusid=3 or factura.fiestatusid is null) and (pedido.fipedstat=1 or pedido.fipedstat is null)  and factura. fdCreacion between @FechaInicio AND @FechaFin</property>
